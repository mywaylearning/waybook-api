      "mysql": {
        "columnName": "email",
        "dataType": "VARCHAR",
        "dataLength": 255,
        "nullable": "N"
      }
    },
    "password": {
      "type": "string",
      "required": true,
      "mysql": {
        "columnName": "password",
        "dataType": "VARCHAR",
        "dataLength": 128,
        "nullable": "N"
      }
    },
    "emailVerified": false,
    "verificationToken": false,
    "status": {
      "type": "string",
      "required": true,
      "default": "A",
      "mysql": {
        "columnName": "status",
        "dataType": "CHAR",
        "dataLength": 1,
        "nullable": "N"
      }
    },
    "created": {
      "type": "date",
      "required": false,
      "mysql": {
